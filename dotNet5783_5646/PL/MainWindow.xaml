<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/backround.jpg"/>
        </Grid.Background>
        <Button Content="Enter" HorizontalAlignment="Center" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="62" Width="102" Click="Button_Click" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFFAFAFA" FontSize="38" Margin="0,295,0,0"/>
        <Button x:Name="For_Manager" Content="For Manager" HorizontalAlignment="Left" Margin="563,83,0,0" VerticalAlignment="Top" Height="56" Width="237" Click="For_Manager_Click" BorderBrush="{x:Null}" Background="{x:Null}" Foreground="#FFEFEFEF" FontSize="24"/>

    </Grid>
</Window>
