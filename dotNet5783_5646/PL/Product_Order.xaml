        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="Product_Order" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/backround2.jpg"/>
        </Grid.Background>
        <Button x:Name="ToProduct" Content="Product" HorizontalAlignment="Left" Margin="547,313,0,0" VerticalAlignment="Top" Click="ToProduct_Click"/>
        <Button x:Name="Order" Content="Order" HorizontalAlignment="Left" Margin="293,316,0,0" VerticalAlignment="Top" Click="Order_Click"/>

    </Grid>
</Window>
